      @media (max-width: 767px) {
        .card1 {
          margin: 20px 0;
        }

        .table-responsive {
          overflow-x: auto;
        }

        .card-body {
          padding: 10px;
        }

        table {
          width: 100%;
          table-layout: fixed;
        }
