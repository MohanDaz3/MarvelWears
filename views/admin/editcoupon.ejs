<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.20/dist/sweetalert2.min.css">

<!-- SweetAlert JS -->
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.20/dist/sweetalert2.all.min.js"></script>



    <script>
      
        
$(document).ready(function() {
  $('.block-button').click(function() {
    var button = $(this);
    var productId = button.data('productid');
    console.log(productId)
    var blockStatus = button.data('blockstatus');
    var url = '';

    if (blockStatus == false) {
      url = '/admin/enableproduct?id=' + productId;
    } else {
      url = '/admin/disableproduct?id=' + productId;
    }

    // Show SweetAlert confirmation dialog
    Swal.fire({
      title: 'Are you sure?',
      text: 'This action cannot be undone.',
      icon: 'warning',
      showCancelButton: true,
      confirmButtonColor: '#3085d6',
      cancelButtonColor: '#d33',
      confirmButtonText: 'Yes'
    }).then((result) => {
      if (result.isConfirmed) {
        // Send Ajax request
        $.ajax({
          url: url,
          type: 'POST',
          success: function(response) {
            console.log(response.message);
            // Update the button text and data attributes
            if (blockStatus == false) {
              button.removeClass('btn-brand').addClass('btn-danger').text('Unblock');
              button.data('blockstatus', true);
            } else {
              button.removeClass('btn-danger').addClass('btn-info').text('Block');
              button.data('blockstatus', false);
            }
            // Show success message
            Swal.fire({
              title: 'Success',
              text: 'Product has been ' + (blockStatus == false ? 'Unblocked' : 'Blocked') + '.',
              icon: 'success',
              timer: 2000,
              showConfirmButton: false
            });
          },
          error: function(error) {
            console.log('Error: ' + error);
          }
        });
      }
    });
  });
});



    </script>

